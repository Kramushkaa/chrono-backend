      "default": "./dist/schemas.js"
    },
    "./types": "./dist/types.d.ts"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "rimraf": "^5.0.5"
  }
}


