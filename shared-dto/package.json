{
  "name": "@chrononinja/dto",
  "version": "0.1.0",
  "description": "Shared DTO schemas and types for Chrononinja FE/BE",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schemas": {
