{
  "name": "@chrononinja/dto",
  "version": "0.3.0",
  "description": "Shared DTO schemas and types for Chrononinja FE/BE",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "default": "./dist/schemas.js"
    },
    "./types": "./dist/types.d.ts",
    "./dtoDescriptors": {
      "types": "./dist/dtoDescriptors.d.ts",
      "default": "./dist/dtoDescriptors.js"
    },
    "./quiz-types": {
      "types": "./dist/quiz-types.d.ts",
      "default": "./dist/quiz-types.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p .",
    "postbuild": "node copy-to-frontend.js",
    "prepare": "npm run build",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "rimraf": "^5.0.5"
  }
}


